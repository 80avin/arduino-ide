{
  "name": "arduino-create-extension",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.1.2",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@types/auth0-js": "8.11.2",
    "@theia/core": "next",
    "@theia/electron": "next",
    "arduino-ide-extension": "0.0.7",
    "auth0-js": "9.11.3",
    "hash.js": "^1.1.7"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "theia": {
    "frontend": {
      "config": {
        "preferences": {
          "files.exclude": {
            "**/.arduino_create": true
          }
        }
      }
    }
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/arduino-create-extension-frontend-module"
    },
    {
      "frontendElectron": "lib/electron-browser/arduino-create-electron-frontend-module",
      "backend": "lib/node/arduino-create-backend-module"
    }
  ]
}
